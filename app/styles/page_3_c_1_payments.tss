	width:'50%',
},

".view-field-ccv":{
	left: '4%',
	width:'45%',
},

".view-field-cp":{
	left: 0,
	width:'40%',
},

".view-field-ville":{
	left: '4%',
	width:'55%',
},

".field":{
	color:'#000',
	width:'100%',
	left:12,
	height:'100%',
	borderRadius:6,
	backgroundColor:'transparent',
    backgroundSelectedColor:'transparent',
    backgroundFocusedColor:'transparent',
	keyboardType:Ti.UI.KEYBOARD_TYPE_DEFAULT,
	returnKeyType:Ti.UI.RETURNKEY_DEFAULT,
	borderStyle:Ti.UI.INPUT_BORDERSTYLE_NONE,
	font:{
		fontSize:26,
		fontFamily:"Wask_New"
	},
	zIndex: 3,
},
".field[platform=ios]":{
	font:{
		fontSize:26,
		fontFamily:"WaskNew"
	},
},

"#CardNumber":{
	keyboardType: Ti.UI.KEYBOARD_TYPE_NUMBER_PAD,
	returnKeyType: Ti.UI.RETURNKEY_NEXT
},

"#CardExp1":{
	width:36,
	keyboardType: Ti.UI.KEYBOARD_TYPE_NUMBER_PAD,
	returnKeyType: Ti.UI.RETURNKEY_NEXT
},

"#CardExp2":{
	width:40	,
	keyboardType: Ti.UI.KEYBOARD_TYPE_NUMBER_PAD,
	returnKeyType: Ti.UI.RETURNKEY_NEXT
},

"#CardCCV":{
	passwordMask:true,
	keyboardType: Ti.UI.KEYBOARD_TYPE_NUMBER_PAD,
	returnKeyType: Ti.UI.RETURNKEY_DONE
},

"#IbanName":{
	returnKeyType: Ti.UI.RETURNKEY_NEXT
},

"#IbanAddr":{
	returnKeyType: Ti.UI.RETURNKEY_NEXT
},

"#IbanVille":{
	returnKeyType: Ti.UI.RETURNKEY_NEXT
},

"#IbanIban":{
