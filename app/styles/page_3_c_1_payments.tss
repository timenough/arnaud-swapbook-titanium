".window":{
	backgroundColor:"white",
	orientationModes: [
		Ti.UI.PORTRAIT
	]
},

".container": {
	top: 80,
	left: Alloy.Globals.backdropImageLeft,
	backgroundColor: "#ffffff",
	layout: 'vertical',
	width: '100%',
	height: Ti.UI.FILL,
},

"#tabBarTop": {
	top: 0,
	width:'100%',
	backgroundColor: Alloy.Globals.BLUE_COLOR,
	height: 41,
	layout: 'horizontal'
},

	".button":{
		left: 0,
		height: 40,
		width:"50%",
		borderRadius: 0,
		color:Alloy.Globals.BLUE_COLOR_v2,
		backgroundColor: "#ffffff",
		borderRadius: 0,
		borderWidth:0,
		textAlign: Ti.UI.TEXT_ALIGNMENT_CENTER,
		verticalAlign: Ti.UI.TEXT_VERTICAL_ALIGNMENT_CENTER,
		font:{
			fontSize: 26,
			fontFamily:"Wask_New"
		},
	},
	".button[platform=ios]":{
		font:{
			fontSize: 22,
			fontFamily:"WaskNew"
		},
	},
	
	".selected":{
		color:"#ffffff",
		backgroundColor: Alloy.Globals.BLUE_COLOR,
	},

	".tabSeparator": {
		top: 0,
		height: 0.5,
		width: '100%',
		backgroundColor: Alloy.Globals.BLUE_COLOR_v2
	},

".content":{
	top: 0,
	left: Alloy.Globals.backdropImageLeft,
	layout: 'vertical',
	width: '100%',
	height: Ti.UI.FILL,
	showVerticalScrollIndicator: false,
	showHorizontalScrollIndicator: false,
},

".card":{
	layout: 'vertical',
	width: '100%',
	top: 16,
	height: 340,
},
		
